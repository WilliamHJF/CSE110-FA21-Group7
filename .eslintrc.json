{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["google", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "no-var": "warn",
    "semi": ["error", "always"],
    "prettier/prettier": "off", // don't show prettier errors as it will be fixed when saved anyway
    "require-jsdoc": "off"
  }
}
